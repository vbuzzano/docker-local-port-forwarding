#!/bin/bash
. `dirname $0`/../.env

cd $STACK_DIR || exit

`dirname $0`/tag-image
if [ ! $? -gt 0 ]; then
  exit $?
fi

# Publish Tag Latest
docker push code28tech/local-port-forwarding:latest || exit

# Publish Tag Version
docker push code28tech/local-port-forwarding:${IMG_VERSION} || exit

echo "Done"
